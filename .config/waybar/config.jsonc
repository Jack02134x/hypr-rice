  "backlight": {
    "device": "acpi_video0",
    "format": "{icon} {percent}",
    "format-icons": ["Û∞™û", "Û∞™ü", "Û∞™†", "Û∞™°", "Û∞™¢", "Û∞™£", "Û∞™§", "Û∞™•"],
    "on-click": "brightnessctl set 25%",
    "on-double-click": "brightnessctl set 50%",
    "on-scroll-up": "brightnessctl set 2%+",
    "on-scroll-down": "brightnessctl set 2%-",
    "tooltip-format": "Backlight"
  },

    "wireplumber": {
        "format": "{icon} {volume}%",
