        "on-click": "pavucontrol",
    },

  "cava": {
    "on-click": "mpv --no-video \"$(find ~/Music -type f \\( -iname '*.mp3' -o -iname '*.opus' -o -iname '*.ogg' -o -iname '*.flac' -o -iname '*.wav' -o -iname '*.m4a' -o -iname '*.aac' \\) | shuf -n 1)\"",
    "on-right-click": "pkill mpv",
    "cava_config": "$HOME/.config/cava/config",
    "framerate": 60,
    "autosens": 1,
    "sensitivity": 0.5,
    "bars": 25,
    "lower_cutoff_freq": 100,
    "higher_cutoff_freq": 10000,
    "method": "pulse",
    "source": "auto",
    "stereo": true,
    "reverse": false,
    "bar_delimiter": 0,
    "monstercat": true,
    "waves": false,
    "noise_reduction": 0.3,
    "input_delay": 1,
    "format-icons": ["‚ñÅ", "‚ñÇ", "‚ñÉ", "‚ñÑ", "‚ñÖ", "‚ñÜ", "‚ñá", "‚ñà"],
    "actions": {}
